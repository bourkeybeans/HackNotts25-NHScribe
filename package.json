{
  "name": "nhscribe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:backend": "python3 -m uvicorn app:app --reload --host 0.0.0.0 --port 8000",
    "start:frontend": "cd front-end && npm start",
    "start": "concurrently --kill-others-on-fail \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "jspdf": "^3.0.3"
  }
}
